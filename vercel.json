{
  "version": 2,
  "builds": [
    { "src": "backend/api/*.js", "use": "@vercel/node" },
  { "src": "public/index.html", "use": "@vercel/static" },
  { "src": "public/favicon.svg", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "backend/api/$1.js" },
  { "src": "/favicon.svg", "dest": "/public/favicon.svg" },
  { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
