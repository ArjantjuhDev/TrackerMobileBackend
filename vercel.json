{
  "version": 2,
  "rewrites": [
    { "source": "/api/:match*", "destination": "/backend/api/:match*.js" }
  ],
  "headers": [
    {
      "source": "/components/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
